#include <stdio.h>

int main(){

//achar o volume que Ã© altura.largura.comprimento.
 int num, num2 , num3 ; 
   printf("altura 1 : ");
   scanf("%d" , &num) ;
   printf("largura 1 : ");
   scanf("%d" , &num2) ;
   printf("comprimento 1 : ");
   scanf("%d" , &num3) ;
   int soma = num*num2*num3;
   printf(" volume : %d " , soma);
}






