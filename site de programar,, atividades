      printf("valor 2 : ");
      scanf("%d" , &num2) ;
      int soma = num+num2;
      printf("soma : %d " , soma );

   
 //char o volume que é altura.largura.comprimento.
  int num, num2 , num3 ; 
  printf("altura 1 : ");
  scanf("%d" , &num) ;
  printf("largura 1 : ");
  scanf("%d" , &num2) ;
  printf("comprimento 1 : ");
  scanf("%d" , &num3) ;
  int soma = num*num2*num3;
  printf(" volume : %d " , soma);


         //imprimir a idade na tela
      int idade= 25 ; // numero inteiro
      
      printf("idade = %d",idade);
     
     
   // somar variavel a e b, e imprimir na tela
      int a= 5; 
